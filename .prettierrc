{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["**/*.scss", "**/*.html"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "tabWidth": 2,
  "trailingComma": "none",
  "plugins": ["prettier-plugin-tailwindcss"]
}
